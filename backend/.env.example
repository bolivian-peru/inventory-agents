# Database
DATABASE_URL=postgres://ifa:password@localhost:5432/ifa

# Redis
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET=your-jwt-secret-min-32-chars-here
TOKEN_ENCRYPTION_KEY=your-32-byte-hex-encryption-key

# Etsy OAuth
ETSY_API_KEY=your-etsy-api-key
ETSY_SHARED_SECRET=your-etsy-shared-secret
ETSY_REDIRECT_URI=http://localhost:8080/etsy/oauth/callback

# OpenClaw
OPENCLAW_GATEWAY_URL=ws://127.0.0.1:18789
OPENCLAW_WORKSPACES_DIR=/root/.openclaw/workspaces

# Server
PORT=8080
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# Anthropic (for agents)
ANTHROPIC_API_KEY=your-anthropic-api-key
